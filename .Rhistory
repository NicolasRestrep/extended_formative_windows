unnest(t) %>%
filter(term == "date") %>%
unnest(data) %>%
mutate(direction = ifelse(estimate < 0, "negative", "positive")) %>%
ggplot(aes(x = date, y = a, color = direction)) +
facet_wrap(~reorder(name, estimate)) +
geom_smooth(method = "lm") +
theme_bw() +
labs(x = "Year", y = "Absolute difference")
